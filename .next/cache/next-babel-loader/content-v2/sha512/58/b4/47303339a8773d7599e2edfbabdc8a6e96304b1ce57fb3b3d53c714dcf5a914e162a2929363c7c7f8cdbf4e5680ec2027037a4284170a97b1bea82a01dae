\n        setCookie(null, \"token\", token, {});\n        axiosAuth.post(\"/auth\", {}).then((res) => {\n          console.log(\"RES =====> \", res);\n          setCont(cont + 1);\n          console.log(\"CONTADOR ===>\", cont);\n          dispatch({\n            type: \"LOGIN\",\n            payload: res.data,\n          });\n        });\n      }\n    });\n  }, []);\n  return <>{children}</>;\n};\n\nexport default FirebaseAuthState;\n"]},"metadata":{},"sourceType":"module"}